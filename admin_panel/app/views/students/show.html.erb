<div id="individualStudentContainer">    
    <div id="studentInfo"> 
        <h1> <%= @student.first_name %> <%= @student.last_name %> </h1>
        <h3> Age: <%= @student.age %> years old </h3>
        <h3><a href="/cohorts/<%= @student_cohort.id%>"> <%= @student_cohort.name %> </h3>
    </div>

    <div id="studentPic">
        <img src="<%= @student.pic %>">
    </div>

</div>
