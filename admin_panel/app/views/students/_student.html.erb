<tr id="student-<%= student.id %>">
  <td><%= student.last_name %></td>
  <td><%= link_to image_tag('icons8-edit-64.png'), edit_student_path(student), remote:true %></td>
  <td><%= link_to image_tag('icons8-trash-can-64.png'), student_path(student), method: :delete, remote:true, data: { confirm: 'Are you sure?' } %></td>
</tr>